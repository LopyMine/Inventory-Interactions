{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_authors}"
    ],
    "contact": {
        "homepage": "https://modrinth.com/mod/${mod_id}",
        "issues": "https://github.com/LopyMine/${mod_id}"
    },
    "license": "${mod_license}",
    "icon": "icon/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
	        "net.lopymine.ipi.InventoryInteractions"
        ],
        "client": [
	        "net.lopymine.ipi.client.InventoryInteractionsClient"
        ],
        "modmenu": [
	        "net.lopymine.ipi.modmenu.ModMenuIntegration"
        ]
    },
    "depends": {
	    "mossylib": ">=${mossylib}",
        "inventory_particles": ">=${inventory_particles}",
        "fabricloader": ">=${fabric_loader}",
        "minecraft": ">=${minecraft}",
        "java": ">=${java}",
        "${fabric_api_id}": "*"
    },
	"accessWidener": "aws/fabric-${minecraft}.accesswidener",
	"mixins": [
		"${fabric_trick_mixin_configs}"
	]
}